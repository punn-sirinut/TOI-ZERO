answer=input().split(' ')
buy=input().split(' ')
if(buy[0]in answer)and(buy[1]in answer):print(1000000)
elif(buy[1]in answer):print(100000)
elif(buy[0]in answer)and(buy[1][2:]in answer[1][2:]):print(2000)
elif(buy[0]in answer)and(buy[1][3:]in answer[1][3:]):print(1000)
elif(buy[1][2:]in answer[1][2:]):print(200)
elif(buy[1][3:]in answer[1][3:]):print(100)
elif(buy[0]in answer):print(20)
else:print(0)